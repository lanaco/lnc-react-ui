{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "semi": "off",
        "quotes": "off",
        "import/export": "off",
        "comma-dangle": "off",
        "space-before-function-paren": "off",
        "no-var": "off",
        "no-unneeded-ternary": "off",
        "react/jsx-no-duplicate-props": "off",
        "multiline-ternary": "off",
        "dot-notation": "off",
        "indent": "off",
        "prefer-const": "off",
        "curly": "off",
        "spaced-comment": "off",
        "react/display-name": "off",
        "yoda": "off",
        "no-prototype-builtins": "off",
        "import/no-named-default": "off",
        "eqeqeq": "off",
        "padded-blocks": "off",
        "eol-last": "off",
        "no-empty": "off",
        "no-multiple-empty-lines": "off",
        "no-tabs": "off"
    }
}
